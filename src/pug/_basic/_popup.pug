            .popup__body.buyProxy__body
                .buyProxy__title Купить мобильные прокси

                form.buyProxy__form.buyProxy-form(action="#" id="buyProxy__form" autocomplete="off" method="post")
                    .buyProxy-form__radio
                        p 1. Выберите тип прокси
                        .buyProxy-form__radio-body
                            input(id="by_services" type='radio' name='typeProxy' value='По сервисам' checked)
                            label(for="by_services") По сервисам
                            input(id="with_expectation" type='radio' name='typeProxy' value='С ожиданием')
                            label(for="with_expectation") С ожиданием
                            input(id="without_waiting" type='radio' name='typeProxy' value='Без ожидания')
                            label(for="without_waiting") Без ожидания
                    .buyProxy-form__select-tuo
                        p 2.  Выберите сервис
                        select.arrow-o
                            option(selected) 1
                            option 2
                            option 3
                            option(data-class='disabled') 4

                        p Ротацию
                        select.arrow-o
                            option 5 минут
                            option 10 минут
                            option(selected) 15 минут

                    .buyProxy-form__select-three
                        p 3.  Выберите страну
                        select.arrow-o
                            option(selected) 1
                            option 2
                            option 3
                            option(data-class='disabled') 4
                        p Город
                        select.arrow-o
                            option 5 минут
                            option 10 минут
                            option(selected) 15 минут
                        p Оператора
                        select.arrow-o
                            option 5 минут
                            option 10 минут
                            option(selected) 15 минут

                    .buyProxy-form__radio
                        p 4.  Выберите тип подключения
                        .buyProxy-form__radio-body
                            input(id="HTTP" type='radio' name='typeProxyСonnection' value='HTTP' checked)
                            label(for="HTTP") HTTP
                            input(id="SOCKS5" type='radio' name='typeProxyСonnection' value='SOCKS5')
                            label(for="SOCKS5") SOCKS5

                    .buyProxy-form__select-tuo
                        p 5.  Укажите период работы
                        select.arrow-o
                            option(selected) 1
                            option 2
                            option 3
                            option(data-class='disabled') 4
                        p
                        select.arrow-o
                            option 5 минут
                            option 10 минут
                            option(selected) 15 минут
                    .buyProxy-form__botton
                        .buyProxy-form__result Итого: <span>150 Р</span>
                        button.button-o.buyProxy__btn(type="submit") отправить заявку
